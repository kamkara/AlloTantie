
<%= form_with(model: product) do |f| %>

  <div class="form-inputs">
    <%= f.text_field :title %>
    <%= f.text_field :describre %>
    <%= f.text_field :price %>
    <%= f.text_field :type %>
    <%= f.text_field :slug %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
